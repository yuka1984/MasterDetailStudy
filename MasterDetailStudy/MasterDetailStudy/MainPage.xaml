<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MasterDetailStudy.MainPage"
             xmlns:pages="clr-namespace:MasterDetailStudy;assembly=MasterDetailStudy">
  <MasterDetailPage.Master>
        <ContentPage Title="Master">
            <StackLayout HorizontalOptions="Fill" VerticalOptions="Center">
                <Button Text="PageA" Clicked="NavigatePageA_OnClicked"/>
                <Button Text="PageB" Clicked="NavigatePageB_OnClicked"/>
            </StackLayout>
        </ContentPage>
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage x:Name="NavigationPage"/>
  </MasterDetailPage.Detail>
</MasterDetailPage>